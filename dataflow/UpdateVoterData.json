{
	"name": "UpdateVoterData",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "RawTableQuery",
						"type": "DatasetReference"
					},
					"name": "RawVoterData"
				},
				{
					"dataset": {
						"referenceName": "tblVoters",
						"type": "DatasetReference"
					},
					"name": "tblVoterData"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "tblVoters",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "ForUpdate"
				},
				{
					"name": "AlterRow1"
				},
				{
					"name": "DC1"
				}
			],
			"scriptLines": [
				"parameters{",
				"     CountyID as integer (2)",
				"}",
				"source(output(",
				"          LASTN as string,",
				"          FIRSTN as string,",
				"          MIDDLEN as string,",
				"          PREFIXN as string,",
				"          SUFFIXN as string,",
				"          CNTYIDNUM as string,",
				"          STNUM as string,",
				"          STDIR as string,",
				"          STNAME as string,",
				"          APT as string,",
				"          CITY as string,",
				"          ZIP as string,",
				"          REGDATE as string,",
				"          BIRTHYEAR as string,",
				"          VOTERUPDT as string,",
				"          VOTERSTAT as string,",
				"          PARTYAFFIL as string,",
				"          LASTVOTE as string,",
				"          PRSID as string,",
				"          Latitude as integer,",
				"          Longitude as integer,",
				"          PHONE as string,",
				"          OEA as integer,",
				"          DTM as timestamp,",
				"          AltPhone as integer,",
				"          RouteID as integer,",
				"          PRECINCT as string,",
				"          PRECINCT_SP as string,",
				"          PRECINCTID as integer,",
				"          SOSIDNUM as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     inputs:['@CountyID' -> ($CountyID)],",
				"     procedureName: 'spS_GetRawVoterDataInput',",
				"     schemaName: 'dbo',",
				"     resultSet: true,",
				"     format: 'procedure') ~> RawVoterData",
				"source(output(",
				"          VOTERID as long,",
				"          LASTN as string,",
				"          FIRSTN as string,",
				"          MIDDLEN as string,",
				"          PREFIXN as string,",
				"          SUFFIXN as string,",
				"          CNTYIDNUM as string,",
				"          STNUM as string,",
				"          STDIR as string,",
				"          STNAME as string,",
				"          APT as string,",
				"          CITY as string,",
				"          ZIP as string,",
				"          REGDATE as string,",
				"          BIRTHYEAR as string,",
				"          VOTERUPDT as string,",
				"          VOTERSTAT as string,",
				"          PARTYAFFIL as string,",
				"          LASTVOTE as string,",
				"          PRSID as string,",
				"          Latitude as decimal(9,6),",
				"          Longitude as decimal(9,6),",
				"          Phone as string,",
				"          OEA as boolean,",
				"          DTM as timestamp,",
				"          AltPhone as string,",
				"          RouteID as integer,",
				"          PRECINCT as integer,",
				"          PRECINCT_SP as integer,",
				"          PRECINCTID as integer,",
				"          SOSIDNUM as string,",
				"          StudentFlag as boolean",
				"     ),",
				"     allowSchemaDrift: false,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> tblVoterData",
				"RawVoterData, tblVoterData exists(RawVoterData@SOSIDNUM==tblVoterData@SOSIDNUM &&(RawVoterData@LASTN!=tblVoterData@LASTN || RawVoterData@STNUM != tblVoterData@STNUM || tblVoterData@STNAME != tblVoterData@STNAME || RawVoterData@CITY != tblVoterData@CITY),",
				"     negate:false,",
				"     broadcast: 'auto')~> ForUpdate",
				"ForUpdate alterRow(updateIf(length(SOSIDNUM)>0)) ~> AlterRow1",
				"AlterRow1 derive(Latitude = toDouble(Latitude * 0) - 9999,",
				"          NEWLAT = toDecimal(99.9999, 9, 6),",
				"          NOROUTE = -1) ~> DC1",
				"DC1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          VOTERID as long,",
				"          LASTN as string,",
				"          FIRSTN as string,",
				"          MIDDLEN as string,",
				"          PREFIXN as string,",
				"          SUFFIXN as string,",
				"          CNTYIDNUM as string,",
				"          STNUM as string,",
				"          STDIR as string,",
				"          STNAME as string,",
				"          APT as string,",
				"          CITY as string,",
				"          ZIP as string,",
				"          REGDATE as string,",
				"          BIRTHYEAR as string,",
				"          VOTERUPDT as string,",
				"          VOTERSTAT as string,",
				"          PARTYAFFIL as string,",
				"          LASTVOTE as string,",
				"          PRSID as string,",
				"          Latitude as decimal(9,6),",
				"          Longitude as decimal(9,6),",
				"          Phone as string,",
				"          OEA as boolean,",
				"          DTM as timestamp,",
				"          AltPhone as string,",
				"          RouteID as integer,",
				"          PRECINCT as integer,",
				"          PRECINCT_SP as integer,",
				"          PRECINCTID as integer,",
				"          SOSIDNUM as string,",
				"          StudentFlag as boolean",
				"     ),",
				"     deletable:false,",
				"     insertable:false,",
				"     updateable:true,",
				"     upsertable:false,",
				"     keys:['SOSIDNUM'],",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     mapColumn(",
				"          LASTN,",
				"          SOSIDNUM,",
				"          FIRSTN,",
				"          RouteID = NOROUTE,",
				"          Latitude = NEWLAT",
				"     )) ~> sink1"
			]
		}
	}
}